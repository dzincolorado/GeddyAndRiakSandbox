<div class="hero-unit">
  <h2>All Getitdones</h2>
  <%- linkTo('Create a new Getitdone', addGetitdonePath, {class: 'btn pull-right'}); %>
</div>

<div id="getitdones-list">
<% if (getitdones) { %>
<% for (var i = 0, ii = getitdones.length; i < ii; i++) { %>
  <div class="row list-item" id="getitdone-<%= getitdones[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(getitdones[i].title, getitdonePath(getitdones[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= getitdones[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>